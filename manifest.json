{

    "manifest_version": 2, 
    "name": "DW: Test",
    "description": "Click on the extension to play a sound!",
    "version": "1.0",
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "allFrames": true,
            "js": ["/js/jquery.js", "/js/content.js"],
            "css": ["/css/style.css"]
       }
     ],
   "background": {
      "scripts": [ "/js/background.js" ]
   },

    "browser_action": {
        "default_icon": {
            "32": "images/kekwicon.png"
        },

    "web_accessible_resources": [ 
        "note.mp3"
     ] 
    }
}